{% extends ':Front:layout.html.twig' %}

{% block title %}{{ parent() }} · {{ 'front.home.block.transparency.title' | trans | capitalize }} · {{ category.name }}{% endblock %}

{% block bodyattr %}class="transparency"{% endblock %}

{% block main %}
<div class="container">
    {#<!-- Breadcrumb -->#}
    <div class="row">
        <div class="col-md-12">
            <div class="banner">
                <div class="overlap">
                    <img class="img-responsive center-block" data-src="holder.js/1140x153/#7EA0B6:#7EA0B6" alt="dummy"/>
                    {% block breadcrumb %}
                        <ol class="breadcrumb">
                            <li><a href="{{ path('demofony2_front_transparency') }}">{{ 'front.home.block.transparency.title' | trans }}</a></li>
                            <li class="active">{{ category.name }}</li>
                        </ol>
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
</div>
{#{% for document in documents %}#}
